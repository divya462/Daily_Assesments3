import java.util.*;
class Prime {
    public static void main(String[] args) {
        int n=new Scanner(System.in).nextInt();
        boolean f=n>1;
        for(int i=2;i<=Math.sqrt(n)&&f;i++) if(n%i==0) f=false;
        System.out.println(n+(f?" is a prime number.":" is not a prime number."));
    }
}

